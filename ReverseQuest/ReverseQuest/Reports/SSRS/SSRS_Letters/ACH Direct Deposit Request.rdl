  <Code>Public Function TextToBarcode(Text As String) As Byte()
           Dim b As System.Drawing.Bitmap
        '   Dim bar As New BarcodeLib.Barcode
           bar.Alignment = BarcodeLib.AlignmentPositions.CENTER
           bar.IncludeLabel = True
           bar.RotateFlipType = Drawing.RotateFlipType.RotateNoneFlipNone
b = bar.Encode(BarcodeLib.TYPE.CODE39Extended, Text, 300, 60)
           Dim bitmapData As Byte() = Nothing
           Using ms As New System.IO.MemoryStream()
               b.Save(ms, System.Drawing.Imaging.ImageFormat.Bmp)
               bitmapData = ms.ToArray()
           End Using
           Return bitmapData
       End Function</Code>
