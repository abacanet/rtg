
    <div id="divContactType">
        <div id="windowContactType" style="display:none">
            <div class="form-group">
                <label for="FirstName">First Name</label>
                <input type="text" class="form-control" name="FirstName" id="FirstName" placeholder="Enter Contact First Name">
            </div>
            <div class="form-group">
                <label for="LastName">Last Name</label>
                <input type="text" class="form-control" name="LastName" id="LastName" placeholder="Enter Contact Last Name">
            </div>
            <div class="form-group">
                <label for="PhoneNumber">Phone Number</label>
                <input type="text" class="form-control" name="PhoneNumber" id="PhoneNumber" placeholder="(555) 555-5555">
            </div>
            <div class="form-group">
                <div id="MSContactType">
                    <div class="demo-section k-content">
                        <select id="multiSelectContactType" multiple="multiple"></select>
                    </div>
                </div>

            </div>
        </div>

        <span id="btnContactType" class="k-button hidden-on-narrow">Contact Type</span>

        <div class="responsive-message"></div>

        <script>

            var ContactTypeDataSource = {
                //   type: "json",
                serverFiltering: false,
                transport: {
                    read: {
                        url: "http://localhost:53176/api/refContactType/GetContactTypes",
                    }
                }
            };
            $(document).ready(function () {
                $("#multiSelectContactType").kendoMultiSelect({
                    height: 200,
                    placeholder: "Select Contact Type...",
                    dataTextField: "contact_type_description",
                    dataValueField: "contact_type_skey",
                    autoBind: false,
                    autoClose: false,
                    dataSource: ContactTypeDataSource,
                    close: function (e) {
                        // e.preventDefault();
                    }
                });
            });


            $("#btnContactType").click(function () {
                $("#windowContactType").data("kendoWindow").open();
            });

            $(document).ready(function () {
                $("#windowContactType").kendoWindow({
                    appendTo: "div#LoanSearchRow",
                   // visible: false,
                    width: "630px",
                    height: "315px",
                    title: "Select Contact Type",
                    actions: ["Close"]         
                }).data("kendoWindow");
            });
        </script>
    </div>


